    "baseUrl": "./src",
    "paths": {
      "@pages": [
        "pages"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@api": [
        "api"
      ],
      "@api/*": [
        "api/*"
      ],
      "@components": [
        "components"
      ],
      "@components/*": [
        "components/*"
      ],
      "@constants": [
        "constants"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@hooks": [
        "hooks"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@utils": [
        "utils"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@stores": [
        "stores"
      ],
      "@stores/*": [
        "stores/*"
      ],
      "@models": [
        "models"
      ],
      "@models/*": [
        "models/*"
